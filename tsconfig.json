{
    "extends": "astro/tsconfigs/strictest",
    "include": [".astro/types.d.ts", "**/*"],
    "exclude": ["dist"],
    "compilerOptions": {
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "verbatimModuleSyntax": true,
        "paths": {
            "@/*": [
                "./src/*"
            ],
        }
    }
}
